\input generic-regression-test.tex\relax

\input tikzlibrarycommutative-diagrams.koinos.code.tex\relax

\START

\TESTEACH{%
    append {\the\SRC} to {\the\TGT} getting {\the\EXPECTED}
}{
    {TGT}   {SRC}   {EXPECTED}  \\
    {foo}   {bar}   {foobar}    \\
    {\foo}  {bar}   {\foo bar}  \\
    {foo}   {\bar}  {foo\bar}   \\
    {\foo}  {\bar}  {\foo\bar}  \\
}{
    \kDAppend\SRC\TGT
    \ASSERT{\the\TGT}{\the\EXPECTED}
}

\END